{
  "private": true,
  "scripts": {
    "dev": "dumi dev",
    "watch": "father-build build --watch",
    "doc:build": "dumi build",
    "doc:deploy": "bash ./scripts/deploy_doc.sh",
    "bootstrap": "lerna bootstrap",
    "build": "father-build build",
    "changelog": "lerna-changelog",
    "clean": "lerna clean -y",
    "test": "umi-test --coverage",
    "debug": "umi-test",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty \"./packages/preset-dumi/src\"",
    "prettier": "prettier -c --write \"**/*\"",
    "publish": "umi-test && npm run changelog && npm run build && lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umijs/dumi.git"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@types/jest": "^24.0.13",
    "@umijs/fabric": "^2.0.0-beta.4",
    "antd": "^4.3.4",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^6.0.3",
    "dumi": "^1.0.0-beta.13",
    "eslint": "^6.7.2",
    "father-build": "^1.14.2",
    "lerna": "^3.6.0",
    "lerna-changelog": "^0.8.2",
    "prettier": "^1.19.1",
    "umi-plugin-githubpages": "^2.0.0",
    "umi-test": "^1.5.12"
  }
}
